	journal = {Biometrics},
	author = {Köllmann, Claudia and Bornkamp, Björn and Ickstadt, Katja},
	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/biom.12193},
	keywords = {Penalty, REML, B-splines, Bayesian semiparametric regression, Shape constrained regression, Unimodality},
	pages = {783--793},
	file = {Full Text PDF:/home/bolker/Documents/zotero_new/storage/M2DVITWZ/Köllmann et al. - 2014 - Unimodal regression using Bernstein–Schoenberg spl.pdf:application/pdf;Snapshot:/home/bolker/Documents/zotero_new/storage/PLFWSPAW/biom.html:text/html},
